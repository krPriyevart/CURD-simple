{
    "version": 2,
    "builds": [
      {
        "src": "*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.js",
        "method": ["GET","POST","PUT","DELETE","PATCH","OPTIONS"],
        "headers": {
          "Access-Control-Allow-Origin": "https://curd-simple-frontend.vercel.app"
        }
      }
    ]
  }
  